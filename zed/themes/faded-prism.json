{
  "$schema": "https://zed.dev/schema/themes/v0.2.0.json",
  "name": "Faded Prism",
  "author": "Ryan Brue",
  "themes": [
    {
      "name": "Faded Prism",
      "appearance": "dark",
      "style": {
        "background.appearance": "opaque",
        "accents": [],
        "border.focused": "#909090",
        "elevated_surface.background": "#181818",
        "surface.background": "#181818",
        "background": "#252525",
        "element.background": "#7bbdff",
        "element.selected": "#4d4d4d",
        "ghost_element.selected": "#4d4d4d",
        "text": "#d8d8d8",
        "status_bar.background": "#181818",
        "title_bar.background": "#181818",
        "toolbar.background": "#252525",
        "tab_bar.background": "#181818",
        "tab.inactive_background": "#181818",
        "tab.active_background": "#181818",
        "panel.background": "#181818",
        "scrollbar.track.background": "#252525",
        "editor.foreground": "#d8d8d8",
        "editor.background": "#252525",
        "editor.gutter.background": "#252525",
        "editor.active_line_number": "#d8d8d8",
        "created": "#6eff69",
        "deleted": "#ff3e3e",
        "error": "#ff3e3e",
        "hint": "#969696ff",
        "info": "#4fd6ff",
        "modified": "#ffea8a",
        "warning": "#ffd932",
        "players": [],
        "syntax": {
          "boolean": {
            "color": "#f7afaf"
          },
          "comment": {
            "color": "#909090"
          },
          "comment.doc": {
            "color": "#909090"
          },
          "constant": {
            "color": "#b8e3ff"
          },
          "function": {
            "color": "#abff8d"
          },
          "keyword": {
            "color": "#ff8d74"
          },
          "number": {
            "color": "#f5c8ff"
          },
          "operator": {
            "color": "#909090"
          },
          "punctuation": {
            "color": "#909090"
          },
          "punctuation.bracket": {
            "color": "#909090"
          },
          "punctuation.delimiter": {
            "color": "#909090"
          },
          "punctuation.list_marker": {
            "color": "#909090"
          },
          "punctuation.special": {
            "color": "#909090"
          },
          "string": {
            "color": "#fffa9f"
          },
          "string.escape": {
            "color": "#fffa9f"
          },
          "string.regex": {
            "color": "#fffa9f"
          },
          "string.special": {
            "color": "#fffa9f"
          },
          "string.special.symbol": {
            "color": "#fffa9f"
          },
          "text.literal": {
            "color": "#fffa9f"
          },
          "type": {
            "color": "#7bbdff"
          },
          "variable": {
            "color": "#d8d8d8"
          },
          "variable.special": {
            "color": "#d8d8d8"
          }
        }
      }
    }
  ]
}
